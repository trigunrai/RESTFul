package com.zensar.entities;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;
import javax.xml.bind.annotation.XmlRootElement;
/*
Author: Trigun Rai
Creation Date: 26th Jul 2019 10:00AM
Modification Date: 26th Jul 2019 10:00AM
Version:1.0
Copyright:Zensar Technnologies. All rights reserved
Description: Represents busines entity product.
It is persistent xlass of hibernate,
value object of application,
domain or application object,
it is also POJO.
*/
@XmlRootElement
@Entity 
@Table(name="PRODUCT")
public class Product {
	
	
	public int getProductid() {
		return productid;
	}
	public void setProductid(int productid) {
		this.productid = productid;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getBrand() {
		return brand;
	}
	public void setBrand(String brand) {
		this.brand = brand;
	}
	public float getPrice() {
		return price;
	}
	public void setPrice(float price) {
		this.price = price;
	}
	public Product(int productid, String name, String brand, float price) {
		super();
		this.productid = productid;
		this.name = name;
		this.brand = brand;
		this.price = price;
	}
	@Override
	public String toString() {
		return "Product [productid=" + productid + ", name=" + name + ", brand=" + brand + ", price=" + price
				+ ", getProductid()=" + getProductid() + ", getName()=" + getName() + ", getBrand()=" + getBrand()
				+ ", getPrice()=" + getPrice() + ", getClass()=" + getClass() + ", hashCode()=" + hashCode()
				+ ", toString()=" + super.toString() + "]";
	}
	@Id
	//@GeneratedValue
	@Column(name="ID")
private int productid;
private String name;
private String brand;
private float price;

public Product() {
	// TODO Auto-generated constructor stub
}
}
